<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dongwha.test02" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>test02</name>
    <description>
    A sample Apache Cordova application that responds to the deviceready event.
  </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
    Apache Cordova Team
  </author>
    <content src="html/LOG/LOG0100.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash height="480" src="res/screen/ios/splash.png" width="320" />
        <splash height="960" src="res/screen/ios/splash@2x.png" width="640" />
        <splash height="1024" src="res/screen/ios/splash-1024h-Portrait.png" width="768" />
        <splash height="2048" src="res/screen/ios/splash-1024h-Portrait@2x.png" width="1536" />
        <splash height="768" src="res/screen/ios/splash-768h-Landscape.png" width="1024" />
        <splash height="1536" src="res/screen/ios/splash-768h-Landscape@2x.png" width="2048" />
        <splash height="1136" src="res/screen/ios/splash-568h@2x.png" width="640" />
        <splash height="1334" src="res/screen/ios/splash-667h@2x.png" width="750" />
        <splash height="2208" src="res/screen/ios/splash-736h-Portrait@3x.png" width="1242" />
        <splash height="1242" src="res/screen/ios/splash-736h-Landscape@3x.png" width="2208" />
        <splash src="res/screen/ios/Default@2x~iphone~anyany.png" />
        <splash src="res/screen/ios/Default@2x~iphone~comcom.png" />
        <splash src="res/screen/ios/Default@3x~iphone~anyany.png" />
        <splash src="res/screen/ios/Default@2x~ipad~anyany.png" />
        <splash src="res/screen/ios/Default@2x~ipad~comany.png" />
    </platform>
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-http" spec="^1.2.0" />
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="^4.5.4" />
</widget>
